<!--            aunt              -->

<rule>
<body>  
CONSTRUCT {
    ?me dbo:aunt ?myaunt .
    [] dbo:weight "0.50"^^xsd:float .
}
WHERE {
    ?brother dbo:aunt ?myaunt .
    ?me dbo:brother ?brother. 
    
    FILTER (?me != ?myaunt)
    FILTER (EXISTS {?myaunt foaf:gender 'female'@en .})

}

</body>
</rule>

<rule>
<body>  
CONSTRUCT {
    ?me dbo:aunt ?myaunt .
    [] dbo:weight "0.50"^^xsd:float .
}
WHERE {
    ?mygrandparent dbo:child ?myaunt . 
    ?me dbo:grandparent ?mygrandparent .
    
    FILTER (?me != ?myaunt)
    FILTER (EXISTS {?myaunt foaf:gender 'female'@en .})
}

</body>
</rule>

<rule>
<body>  
CONSTRUCT {
    ?me dbo:aunt ?myaunt .
    [] dbo:weight "1.00"^^xsd:float .
}
WHERE {
    ?me dbo:parent ?myparent . 
    ?myparent dbo:sister ?myaunt .
    
    FILTER (?me != ?myaunt)
    FILTER (EXISTS {?myaunt foaf:gender 'female'@en .})
}

</body>
</rule>

<rule>
<body>  
