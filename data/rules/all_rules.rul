    [] dbo:weight "0.25"^^xsd:float .
}
WHERE {
    ?me dbo:grandparent ?mygrandparent .
    ?myparent dbo:parent ?mygrandparent .

    FILTER (?me != ?myparent)
}

</body>
</rule>


