<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY rule   "http://ns.inria.fr/corese/rule/"> 
  <!ENTITY rdf    "http://www.w3.org/1999/02/22-rdf-syntax-ns">
  <!ENTITY rdfs   "http://www.w3.org/2000/01/rdf-schema">
  <!ENTITY xsd    "http://www.w3.org/2001/XMLSchema#">
  <!ENTITY spin   "http://spinrdf.org/sp#">
  <!ENTITY owl    "http://www.w3.org/2002/07/owl#">
  <!ENTITY dbo    "http://dbpedia.org/ontology/">
]>

<rdf:RDF 
    xmlns:rdf  = "&rdf;#"
    xmlns:rdfs = "&rdfs;#"
    xmlns:xsd  = "&xsd;" 
    xmlns:rule = "&rule;#"
    xmlns:owl  = "&owl;"
    xmlns:sp   = "&spin;"
    xmlns      = "&rule;"
    xmlns:dbo  = "&dbo;"
>

<rule>
<body>  
CONSTRUCT {
    ?myparent dbo:child ?me .
    [] dbo:weight "0.99"^^xsd:float .
}
WHERE {
    ?myparent dbo:child ?mysister .
    ?me dbo:sister ?mysister .

    FILTER (?myparent != ?paul)
}

</body>
</rule> 


<rule>
<body>  
CONSTRUCT {
    ?myparent dbo:child ?me .
    [] dbo:weight "0.99"^^xsd:float .
}
WHERE {
    ?myuncle dbo:brother ?myparent .
    ?me dbo:uncle ?myuncle .

    FILTER (?myparent != ?me)
}

</body>
</rule> 

